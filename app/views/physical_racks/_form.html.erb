<%= simple_form_for([@building, @physical_rack]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
		<%= f.input :name %>
		<%= f.input :ru_size %>
		<%= f.association :room, :collection => @building.rooms %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
